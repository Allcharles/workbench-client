{
  "name": "bawclient",
  "author": {
    "name": "Charles Alleman",
    "email": "charles@alleman.com.au"
  },
  "description": "Client-side website for online ecoacoustic analysis.",
  "version": "3.0.0",
  "scripts": {
    "build": "nx build --prod",
    "e2e": "nx e2e",
    "e2e:debug": "node --max_old_space_size=4096 --inspect-brk ./node_modules/protractor/bin/protractor e2e/protractor.conf.js",
    "lint": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js .",
    "ng": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "nx": "nx",
    "start": "nx serve --host 0.0.0.0 --disableHostCheck",
    "stats": "nx build --stats-json && webpack-bundle-analyzer dist/workbench-client/browser/stats.json",
    "test": "npm run test:chrome",
    "test:chrome": "nx test -- --code-coverage --browsers Chrome",
    "test:firefox": "nx test -- --code-coverage --browsers Firefox",
    "test:edge": "nx test -- --code-coverage --browsers Edge",
    "test:all": "nx test -- --code-coverage --browsers Firefox,Chrome,Edge",
    "test:pipeline": "nx test -- --watch=false --code-coverage",
    "dev:ssr": "nx run workbench-client:serve-ssr",
    "serve:ssr": "node dist/workbench-client/server/main.js",
    "build:ssr": "nx build --prod && ng run workbench-client:server:production",
    "prerender": "nx run workbench-client:prerender",
    "postinstall": "node --max_old_space_size=4096 ./decorate-angular-cli.js",
    "webdriver": "node ./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.11",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.11",
    "@angular/compiler": "^11.2.11",
    "@angular/core": "^11.2.11",
    "@angular/forms": "^11.2.11",
    "@angular/google-maps": "^11.2.13",
    "@angular/localize": "^11.2.9",
    "@angular/platform-browser": "^11.2.11",
    "@angular/platform-browser-dynamic": "^11.2.11",
    "@angular/platform-server": "^11.2.11",
    "@angular/router": "^11.2.11",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^9.1.1",
    "@nguniversal/express-engine": "^11.2.1",
    "@ngx-formly/bootstrap": "^5.10.18",
    "@ngx-formly/core": "^5.10.18",
    "@ngx-loading-bar/core": "^5.1.2",
    "@ngx-loading-bar/http-client": "^5.1.2",
    "@swimlane/ngx-datatable": "^19.0.0",
    "@vvo/tzdb": "^6.7.0",
    "bootstrap": "^4.6.0",
    "express": "^4.17.1",
    "filesize": "^6.3.0",
    "full-icu": "^1.3.4",
