{
  "name": "baw-client",
  "author": {
    "name": "Charles Alleman",
    "email": "charles@alleman.com.au"
  },
  "description": "Client-side website for online ecoacoustic analysis.",
  "version": "3.0.0",
  "scripts": {
    "build": "ng build --prod",
    "code-coverage": "npm test -- --watch=false && http-server -c-1 -o -p 9875 ./coverage/workbench-client",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/locale",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "stats": "ng build --stats-json",
    "test": "npm run test:chrome",
    "test:chrome": "ng test -- --code-coverage --browsers Chrome",
    "test:firefox": "ng test -- --code-coverage --browsers Firefox",
    "test:edge": "ng test -- --code-coverage --browsers Edge",
    "test:opera": "ng test -- --code-coverage --browsers Opera",
    "test:all": "ng test -- --code-coverage --browsers Firefox,Chrome,Edge",
    "test:pipeline": "ng test -- --watch=false --code-coverage --browsers FirefoxHeadless,ChromeHeadless"
  },
  "private": true,
