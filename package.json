{
  "name": "baw-client",
  "author": {
    "name": "Charles Alleman",
    "email": "charles@alleman.com.au"
  },
  "description": "Client-side website for online ecoacoustic analysis.",
  "version": "3.0.0",
  "scripts": {
    "build": "ng build --prod",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/locale",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "stats": "ng build --stats-json && webpack-bundle-analyzer dist/workbench-client/stats.json",
    "test": "npm run test:chrome",
    "test:chrome": "ng test -- --code-coverage --browsers Chrome",
    "test:firefox": "ng test -- --code-coverage --browsers Firefox",
    "test:edge": "ng test -- --code-coverage --browsers Edge",
    "test:opera": "ng test -- --code-coverage --browsers Opera",
    "test:all": "ng test -- --code-coverage --browsers Firefox,Chrome,Edge",
    "test:pipeline:windows": "ng test -- --watch=false --code-coverage --browsers FirefoxHeadless,ChromeHeadless,EdgeHeadless",
    "test:pipeline:linux": "ng test -- --watch=false --code-coverage --browsers FirefoxHeadless,ChromeHeadless",
    "test:pipeline:mac": "ng test -- --watch=false --code-coverage --browsers FirefoxHeadless,ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@agm/snazzy-info-window": "^1.1.0",
    "@angular/animations": "^9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/localize": "~9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@ngx-formly/bootstrap": "^5.7.2",
    "@ngx-formly/core": "^5.7.2",
    "@ngx-loading-bar/core": "^5.0.0",
    "@ngx-loading-bar/http-client": "^5.0.0",
    "@swimlane/ngx-datatable": "^17.0.0",
    "bootstrap": "^4.5.0",
    "filesize": "^6.1.0",
    "full-icu": "^1.3.1",
