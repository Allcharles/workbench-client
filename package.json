    "just-camel-case": "^4.0.2",
    "just-snake-case": "^1.2.1",
    "luxon": "^1.27.0",
    "ngx-captcha": "^9.0.1",
    "ngx-cookie-service": "^11.0.2",
    "ngx-device-detector": "^2.1.1",
    "ngx-line-truncation": "^1.7.0",
    "ngx-toastr": "^13.2.1",
