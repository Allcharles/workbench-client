    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@nguniversal/express-engine": "^11.0.0",
    "@ngx-formly/bootstrap": "^5.10.6",
    "@ngx-formly/core": "^5.10.6",
    "@ngx-loading-bar/core": "^5.1.0",
    "@ngx-loading-bar/http-client": "^5.1.0",
    "@swimlane/ngx-datatable": "^18.0.0",
