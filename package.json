    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.11",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.1.1",
    "@ngneat/spectator": "^7.1.0",
